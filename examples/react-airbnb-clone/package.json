    "@types/react": "18.3.12",
    "@types/react-date-range": "^1.4.9",
    "@types/react-dom": "18.3.1",
    "@types/uuid": "^10.0.0",
    "@webpack-cli/serve": "^2.0.5",
    "autoprefixer": "^10.4.20",
