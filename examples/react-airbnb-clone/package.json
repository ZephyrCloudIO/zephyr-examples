    "date-fns": "^4.1.0",
    "eslint": "9.15.0",
    "leaflet": "^1.9.4",
    "match-sorter": "^8.0.0",
    "path-browserify": "^1.0.1",
    "query-string": "^9.1.1",
    "react": "18.3.1",
    "react-date-range": "^2.0.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-hook-form": "^7.53.2",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.3.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^7.0.1",
    "react-select": "^5.8.3",
    "react-spinners": "^0.14.1",
    "sort-by": "^1.2.0",
    "swr": "^2.2.5",
    "util": "^0.12.5",
    "uuid": "^11.0.3",
    "world-countries": "^5.0.0",
    "zustand": "^5.0.1"
