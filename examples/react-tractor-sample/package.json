{
  "name": "tractor-sample-root",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many -t serve --parallel=5 -p team-blue team-red team-green",
    "build": "nx run-many -t build --parallel=1 --skip-nx-cache -p team-blue team-red team-green",
    "serve:host": "nx run host:serve",
    "build:host": "nx run host:build:production --skip-nx-cache",
    "serve:team-red": "nx run team-red:serve",
    "build:team-red": "nx run team-red:build:production --skip-nx-cache",
    "serve:team-blue": "nx run team-blue:serve",
    "build:team-blue": "nx run team-blue:build:production --skip-nx-cache",
    "serve:team-green": "nx run team-green:serve",
    "build:team-green": "nx run team-green:build:production --skip-nx-cache  --watch"
  },
  "private": true,
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@nx/react": "^18.2.4",
    "@nx/webpack": "^18.2.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^8.0.1",
    "@swc-node/register": "~1.6.7",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.85",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.2",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.9",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "babel-jest": "^29.4.1",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest-environment-jsdom": "^29.4.1",
    "nx": "^18.2.4",
    "react-refresh": "^0.10.0",
    "swc-loader": "^0.2.6",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "^5.4.5",
    "url-loader": "^4.1.1",
    "zephyr-webpack-plugin": "^0.0.12"
  }
}
