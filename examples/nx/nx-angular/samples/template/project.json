{
  "name": "template",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "samples/template",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "nx run template-host:serve",
            "color": "blue"
          },
          {
            "command": "nx run template-remote:serve",
            "color": "green"
          }
        ],
        "parallel": true
      }
    }
  },
  "tags": []
}
