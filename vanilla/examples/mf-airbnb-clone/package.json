{
  "name": "mf-airbnb-clone",
  "workspaces": [
    "./apps/*"
  ],
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "build": "pnpm build-remotes && pnpm build-host",
    "build-remotes": "pnpm --filter=./apps/* --filter=!airbnb-react-host --parallel run build",
    "build-host": "pnpm --filter=airbnb-react-host run build",
    "start": "pnpm --filter=./apps/* --parallel run start",
    "start-remotes": "pnpm --filter=./apps/* --filter=!airbnb-react-host --parallel run start",
    "start-host": "pnpm --filter=airbnb-react-host run start"
  },
  "dependencies": {
    "concurrently": "^9.2.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.39.0",
    "leaflet": "^1.9.4",
    "match-sorter": "^8.1.0",
    "path-browserify": "^1.0.1",
    "query-string": "^9.3.1",
    "react": "^19.2.0",
    "react-date-range": "^2.0.1",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.66.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.9.5",
    "react-select": "^5.10.2",
    "react-spinners": "^0.17.0",
    "sort-by": "^1.2.0",
    "swr": "^2.3.6",
    "util": "^0.12.5",
    "uuid": "^13.0.0",
    "world-countries": "^5.1.0",
    "zephyr-webpack-plugin": "latest",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@module-federation/enhanced": "^0.21.2",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-date-range": "^1.4.10",
    "@types/react-dom": "^19.2.2",
    "@types/uuid": "^11.0.0",
    "@webpack-cli/serve": "^3.0.1",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.4",
    "http-server": "^14.1.1",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "rimraf": "^6.1.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
