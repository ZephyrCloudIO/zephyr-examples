{
  "name": "rspack-hmr",
  "scripts": {
    "install-all": "concurrently \"pnpm i --filter @rspack-hmr/rspack-host\" \"pnpm i --filter @rspack-hmr/app2\" \"pnpm i --filter @rspack-hmr/rspack-runhost\"",
    "start": "pnpm run --filter @rspack-hmr/* dev",
    "build": "pnpm run --filter @rspack-hmr/* build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}